{
  "name": "noteflow-backend",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "app.js",
  "scripts": {
    "start": "set NODE_ENV=development && npx nodemon ./src/app.js",
    "start-prod": "set NODE_ENV=production && npx nodemon ./src/app.js",
    "test": "mocha tests --exit --recursive ",
    "test2": "mocha tests --exit --recursive --grep \"colab list handling\"",
    "coverage": "nyc mocha tests --exit --recursive",
    "c8": "c8  --exclude \"src/lib/**\" --exclude \"src/lib/**\" --exclude \"src/app.js\" --exclude \"tests\" --exclude \"src/routes/ws-router.js\" npm run test",
    "format": "prettier --config .prettierrc \"**/*.+(js|json)\" --write",
    "lint": "eslint src/**/*.js --fix && eslint tests/**/*.js --fix",
    "db:migrate": "knex --knexfile src/model/postgres/knexfile.js migrate:latest",
    "db:seed": "knex --knexfile src/model/postgres/knexfile.js seed:run",
    "db:rollback": "knex --knexfile src/model/postgres/knexfile.js migrate:rollback",
    "mongo:seed": "node src/model/mongodb/seeds/node-repo.js",
    "check:migrate": "node ./check_migrate.js"
  },
  "author": "sdm-group-6",
